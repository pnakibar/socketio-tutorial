{
  "name": "Web Sockets Playground",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf tmp",
    "prebuild": "npm run clean",
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore",
    "test": "npm run test_setup && NODE_ENV=testing mocha ./test/* --compilers js:babel-register --recursive",
    "dev": "./node_modules/.bin/nodemon src/index.js --exec ./node_modules/.bin/babel-node --presets es2015,stage-0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnakibar/websockets-playground.git"
  },
  "keywords": [
    "express",
    "mocha",
    "es2015",
    "es6",
    "babel"
  ],
  "author": "Pedro Nakibar <pedro@getty.io>",
  "license": "ISC",
  "pre-commit": [
    "lint",
    "test",
    "build"
  ],
  "homepage": "https://github.com/pnakibar/websockets-playground#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "del": "^2.2.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-getty": "0.0.3",
    "eslint-plugin-import": "^1.10.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "should": "^9.0.2",
    "source-list-map": "^0.1.6",
    "source-map": "^0.5.6",
    "stats-webpack-plugin": "^0.3.1",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^4.0.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "connect": "^3.4.1",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "socket.io": "^1.4.8"
  }
}
